<UserControl x:Class="StatsApp.Header"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:StatsApp"
             mc:Ignorable="d"
             d:DesignHeight="80" d:DesignWidth="1200"
             >

    <Grid Background="Red" Height="80" Width="1200" VerticalAlignment="Center">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center"  Height="110" >
            <Button Width="150" Height="60" Click="InitialPage"></Button>
            <Button Width="150" Height="60" Click="StatsPage"></Button>
        </StackPanel>
    </Grid>
</UserControl>
